{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/.cache/src/index.d.ts",

  "apiReport": {
    "enabled": false
  },

  "docModel": {
    "enabled": false
  },

  "dtsRollup": {
    "enabled": true
  },

  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-forgotten-export": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      },
      "tsdoc-param-tag-missing-hyphen": {
        "logLevel": "none"
      }
    }
  }
}
